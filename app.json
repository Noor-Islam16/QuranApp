{
  "expo": {
    "name": "Al-Quran",
    "slug": "Al-Quran",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/quranAppIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/quranAppIcon.png",
        "backgroundColor": "#029688"
      },
      "package": "com.asrtech123.QuranApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/quranAppIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#029688"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a31e7730-fb66-4998-90e4-2f78955567c8"
      }
    }
  }
}